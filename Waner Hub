    TweenService:Create(Background, TweenInfo.new(0.8), {BackgroundTransparency = 1}):Play()
    TweenService:Create(Blur, TweenInfo.new(1), {Size = 0}):Play()
    wait(1.2)
    
    Blur:Destroy()
    ScreenGui:Destroy()
    
    print("‚úÖ Loading Screen completed")
    return true
end

