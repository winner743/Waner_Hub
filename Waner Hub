local currentSpeed = 1

-- ============================================================================
-- ADVANCED GOD MODE SYSTEM WITH MULTIPLE PROTECTION LAYERS
-- ============================================================================

local godModeConnections = {}
local originalHealth = nil

local function enableGodMode()
    godModeEnabled = true
    
    for _, conn in pairs(godModeConnections) do
        pcall(function() conn:Disconnect() end)
    end
    godModeConnections = {}
    
    local function setupGodMode(character)
        if not character then return end
        
        local humanoid = character:WaitForChild("Humanoid", 5)
        if not humanoid then return end
        
        if not originalHealth then
            originalHealth = humanoid.MaxHealth
        end
        
        -- Multiple protection layers
        humanoid.MaxHealth = math.huge
        humanoid.Health = math.huge
        
        -- Block health changes
        local healthConn = humanoid:GetPropertyChangedSignal("Health"):Connect(function()
            if godModeEnabled and humanoid.Health < math.huge then
                humanoid.Health = math.huge
            end
        end)
        
        local maxHealthConn = humanoid:GetPropertyChangedSignal("MaxHealth"):Connect(function()
            if godModeEnabled and humanoid.MaxHealth < math.huge then
                humanoid.MaxHealth = math.huge
                humanoid.Health = math.huge
            end
        end)
        
        -- Block damage events
        local damageConn = humanoid.HealthChanged:Connect(function(health)
            if godModeEnabled and health < humanoid.MaxHealth then
                humanoid.Health = math.huge
            end
        end)
        
        table.insert(godModeConnections, healthConn)
        table.insert(godModeConnections, maxHealthConn)
        table.insert(godModeConnections, damageConn)
    end
    
    if player.Character then
        setupGodMode(player.Character)
    end
    
    local charConn = player.CharacterAdded:Connect(function(character)
        if godModeEnabled then
            task.wait(1)
            setupGodMode(character)
        end
    end)
    
    table.insert(godModeConnections, charConn)
    print("üõ°Ô∏è Advanced God Mode: ENABLED")
end

local function disableGodMode()
    godModeEnabled = false
    
    for _, conn in pairs(godModeConnections) do
        pcall(function() conn:Disconnect() end)
    end
    godModeConnections = {}
    
    if player.Character then
        local humanoid = player.Character:FindFirstChild("Humanoid")
        if humanoid and originalHealth then
            humanoid.MaxHealth = originalHealth
            humanoid.Health = originalHealth
        end
    end
    
    print("üõ°Ô∏è Advanced God Mode: DISABLED")
end

-- ============================================================================
-- ADVANCED BYPASS ANTI-CHEAT SYSTEM
-- ============================================================================

local bypassConnections = {}

local function enableBypassAntiCheat()
    bypassAntiCheat = true
    
    for _, conn in pairs(bypassConnections) do
        pcall(function() conn:Disconnect() end)
    end
    bypassConnections = {}
    
    local function setupBypass(character)
        if not character then return end
        
        local humanoid = character:WaitForChild("Humanoid", 5)
        if not humanoid then return end
        
        -- Block anti-cheat remotes
        local function blockRemotes()
            for _, remote in pairs(ReplicatedStorage:GetDescendants()) do
                if remote:IsA("RemoteEvent") or remote:IsA("RemoteFunction") then
                    local name = remote.Name:lower()
                    if name:find("anticheat") or name:find("speed") or name:find("hack") or name:find("exploit") then
                        local oldFireServer = remote.FireServer
                        if oldFireServer then
                            remote.FireServer = function(...)
                                if bypassAntiCheat then
                                    return
                                end
                                return oldFireServer(...)
                            end
                        end
                        
                        local oldInvokeServer = remote.InvokeServer
                        if oldInvokeServer then
                            remote.InvokeServer = function(...)
                                if bypassAntiCheat then
                                    return
                                end
                                return oldInvokeServer(...)
                            end
                        end
                    end
                end
            end
        end
        
        blockRemotes()
        
        local remoteConn = ReplicatedStorage.DescendantAdded:Connect(function(descendant)
            if bypassAntiCheat then
                task.wait(0.1)
                blockRemotes()
            end
        end)
        
        table.insert(bypassConnections, remoteConn)
    end
    
    if player.Character then
        setupBypass(player.Character)
    end
    
    local charConn = player.CharacterAdded:Connect(function(character)
        if bypassAntiCheat then
            task.wait(1)
            setupBypass(character)
        end
    end)
    
    table.insert(bypassConnections, charConn)
    print("üîì Advanced Bypass Anti-Cheat: ENABLED")
end

local function disableBypassAntiCheat()
    bypassAntiCheat = false
    
    for _, conn in pairs(bypassConnections) do
        pcall(function() conn:Disconnect() end)
    end
    bypassConnections = {}
    
    print("üîì Advanced Bypass Anti-Cheat: DISABLED")
end

-- ============================================================================
-- ADVANCED SPEED SYSTEM WITH SMOOTH TRANSITIONS
-- ============================================================================

local function setSpeed(multiplier)
    currentSpeed = multiplier
    
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        local humanoid = player.Character.Humanoid
        local targetSpeed = 16 * multiplier
        
        if bypassAntiCheat then
            -- Gradual speed change to avoid detection
            local steps = math.max(1, math.abs(targetSpeed - humanoid.WalkSpeed) / 3)
            for i = 1, steps do
                if not player.Character or not player.Character:FindFirstChild("Humanoid") then break end
                
                local progress = i / steps
                local newSpeed = humanoid.WalkSpeed + (targetSpeed - humanoid.WalkSpeed) * progress
                
                humanoid.WalkSpeed = newSpeed
                task.wait(0.05)
            end
        else
            humanoid.WalkSpeed = targetSpeed
        end
    end
end

-- Apply speed on character spawn
player.CharacterAdded:Connect(function(character)
    task.wait(1)
    if character:FindFirstChild("Humanoid") then
        setSpeed(currentSpeed)
    end
end)

-- ============================================================================
-- ADVANCED NOCLIP SYSTEM
-- ============================================================================

local noclipConnection
local noclipParts = {}

local function enableNoclip()
    noclipEnabled = true
    
    if noclipConnection then noclipConnection:Disconnect() end
    
    local function applyNoclip()
        if not player.Character then return end
        
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide then
                part.CanCollide = false
                noclipParts[part] = true
            end
        end
    end
    
    applyNoclip()
    
    noclipConnection = RunService.Stepped:Connect(function()
        if noclipEnabled then
            applyNoclip()
        end
    end)
    
    print("üëª Advanced Noclip: ENABLED")
end

local function disableNoclip()
    noclipEnabled = false
    
    if noclipConnection then noclipConnection:Disconnect() end
    
    for part, _ in pairs(noclipParts) do
        if part and part.Parent then
            part.CanCollide = true
        end
    end
    noclipParts = {}
    
    print("üëª Advanced Noclip: DISABLED")
end

-- ============================================================================
-- ADVANCED AUTO COLLECT SYSTEM WITH SMART DETECTION
-- ============================================================================

local autoCollectConnection
local autoCollectSettings = {
    Vitamins = true,
    Lockpick = true,
    FlashlightPickup = true,
    Lighter = true,
    Candle = true,
    Crucifix = true,
    Bandage = true,
    SkeletonKey = true,
    Shakelight = true,
    Key = true,
    Book = true,
    Paper = true,
    Scanner = true,
    ElectricalKeyCard = true
}

local function autoCollectItems()
    if not autoCollectEnabled then return end
    
    local collectedCount = 0
    
    local function collectFromContainer(container)
        for _, item in pairs(container:GetDescendants()) do
            if collectedCount >= 20 then break end
            
            for itemName, enabled in pairs(autoCollectSettings) do
                if enabled and item:IsA("Model") and (item.Name == itemName or item.Name:find(itemName)) then
                    local prompt = item:FindFirstChildOfClass("ProximityPrompt", true)
                    if not prompt then
                        for _, descendant in pairs(item:GetDescendants()) do
                            if descendant:IsA("ProximityPrompt") and descendant.Enabled then
                                prompt = descendant
                                break
                            end
                        end
                    end
                    
                    if prompt and prompt.Enabled then
                        local itemPart = item:FindFirstChildWhichIsA("BasePart", true)
                        if itemPart and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            local distance = (player.Character.HumanoidRootPart.Position - itemPart.Position).Magnitude
                            
                            if distance <= (prompt.MaxActivationDistance or 15) then
                                pcall(function()
                                    fireproximityprompt(prompt)
                                    collectedCount = collectedCount + 1
                                end)
                                task.wait(0.1)
                            end
                        end
                    end
                end
            end
        end
    end
    
    collectFromContainer(workspace)
    
    local currentRooms = workspace:FindFirstChild("CurrentRooms")
    if currentRooms then
        collectFromContainer(currentRooms)
    end
end

local function enableAutoCollect()
    autoCollectEnabled = true
    
    if autoCollectConnection then autoCollectConnection:Disconnect() end
    
    autoCollectConnection = RunService.Heartbeat:Connect(function()
        if autoCollectEnabled then
            autoCollectItems()
        end
    end)
    
    print("üì¶ Advanced Auto Collect: ENABLED")
end

local function disableAutoCollect()
    autoCollectEnabled = false
    
    if autoCollectConnection then autoCollectConnection:Disconnect() end
    
    print("üì¶ Advanced Auto Collect: DISABLED")
end

-- ============================================================================
-- ENHANCED ESP SYSTEM WITH ADVANCED FEATURES & LIGHT EFFECTS
-- ============================================================================

-- Create enhanced ESP with light effect and animations
local function createAdvancedESP(obj, color, txt, espType)
    if not obj or obj:FindFirstChild("WenerESP") then return nil end
    
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "WenerESP"
    billboard.Adornee = obj
    billboard.Size = UDim2.new(0, 180, 0, 90)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true
    billboard.Parent = obj
    
    -- Background frame with gradient
    local bgFrame = Instance.new("Frame")
    bgFrame.Size = UDim2.new(1, 0, 1, 0)
    bgFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    bgFrame.BackgroundTransparency = 0.2
    bgFrame.BorderSizePixel = 0
    bgFrame.Parent = billboard
    
    local bgCorner = Instance.new("UICorner")
    bgCorner.CornerRadius = UDim.new(0, 10)
    bgCorner.Parent = bgFrame
    
    local bgGradient = Instance.new("UIGradient")
    bgGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(15, 15, 25)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(30, 30, 40))
    })
    bgGradient.Rotation = 90
    bgGradient.Parent = bgFrame
    
    -- Main label with enhanced styling
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 0.65, 0)
    label.BackgroundTransparency = 1
    label.Text = txt
    label.TextColor3 = color
    label.TextStrokeTransparency = 0
    label.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
    label.TextScaled = true
    label.Font = Enum.Font.GothamBold
    label.Parent = bgFrame
    
    -- Distance label with icon
    local distanceLabel = Instance.new("TextLabel")
    distanceLabel.Size = UDim2.new(1, 0, 0.35, 0)
    distanceLabel.Position = UDim2.new(0, 0, 0.65, 0)
    distanceLabel.BackgroundTransparency = 1
    distanceLabel.Text = "üìè 0m"
    distanceLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    distanceLabel.TextStrokeTransparency = 0
    distanceLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
    distanceLabel.TextScaled = true
    distanceLabel.Font = Enum.Font.Gotham
    distanceLabel.Parent = bgFrame
    
    -- Special effects based on ESP type
    if espType == "book" then
        -- Enhanced light effect for books
        local lightEffect = Instance.new("PointLight")
        lightEffect.Brightness = 2.5
        lightEffect.Range = 20
        lightEffect.Color = Color3.fromRGB(255, 255, 100)
        lightEffect.Parent = obj
        
        -- Pulsing light animation
        task.spawn(function()
            while lightEffect and lightEffect.Parent do
                TweenService:Create(lightEffect, TweenInfo.new(1.2, Enum.EasingStyle.Sine), {Brightness = 0.8}):Play()
                wait(1.2)
                TweenService:Create(lightEffect, TweenInfo.new(1.2, Enum.EasingStyle.Sine), {Brightness = 2.5}):Play()
                wait(1.2)
            end
        end)
        
        -- Book glow effect
        local selectionBox = Instance.new("SelectionBox")
        selectionBox.Adornee = obj
        selectionBox.Color3 = Color3.fromRGB(255, 255, 0)
        selectionBox.LineThickness = 0.2
        selectionBox.Transparency = 0.5
        selectionBox.Parent = obj
        
    elseif espType == "figure" then
        -- Warning effect for Figure
        local warningLight = Instance.new("PointLight")
        warningLight.Brightness = 3
        warningLight.Range = 25
        warningLight.Color = Color3.fromRGB(255, 0, 0)
        warningLight.Parent = obj
        
        -- Flashing red light
        task.spawn(function()
            while warningLight and warningLight.Parent do
                warningLight.Enabled = true
                wait(0.3)
                warningLight.Enabled = false
                wait(0.3)
            end
        end)
        
    elseif espType == "door" then
        -- Door outline effect
        local selectionBox = Instance.new("SelectionBox")
        selectionBox.Adornee = obj
        selectionBox.Color3 = Color3.fromRGB(0, 255, 0)
        selectionBox.LineThickness = 0.15
        selectionBox.Transparency = 0.7
        selectionBox.Parent = obj
    end
    
    -- Fade in animation
    bgFrame.BackgroundTransparency = 1
    label.TextTransparency = 1
    distanceLabel.TextTransparency = 1
    
    TweenService:Create(bgFrame, TweenInfo.new(0.5), {BackgroundTransparency = 0.2}):Play()
    TweenService:Create(label, TweenInfo.new(0.5), {TextTransparency = 0}):Play()
    TweenService:Create(distanceLabel, TweenInfo.new(0.5), {TextTransparency = 0}):Play()
    
    -- Update distance
    task.spawn(function()
        while billboard and billboard.Parent do
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local distance = (player.Character.HumanoidRootPart.Position - obj.Position).Magnitude
                distanceLabel.Text = "üìè " .. math.floor(distance) .. "m"
            end
            wait(0.5)
        end
    end)
    
    return billboard
end

-- ============================================================================
-- ENHANCED DOOR ESP
--============================================================================

local doorConnection

local function getDoorNumber(door)
    -- Multiple methods to detect door number
    local doorNumber = "?"
    
    -- Method 1: Check door name
    if door.Name:match("%d+") then
        doorNumber = door.Name:match("%d+")
    end
    
    -- Method 2: Check parent room
    local parent = door.Parent
    if parent and parent.Name:match("%d+") then
        doorNumber = parent.Name:match("%d+")
    end
    
    -- Method 3: Check for NumberValue
    local numberValue = door:FindFirstChild("RoomNumber") or door:FindFirstChildOfClass("NumberValue")
    if numberValue and numberValue.Value then
        doorNumber = tostring(numberValue.Value)
    end
    
    -- Method 4: Check CurrentRooms structure
    local currentRooms = workspace:FindFirstChild("CurrentRooms")
    if currentRooms then
        for _, room in pairs(currentRooms:GetChildren()) do
            if room:FindFirstChild("Door") == door or door:IsDescendantOf(room) then
                if room.Name:match("%d+") then
                    doorNumber = room.Name:match("%d+")
                    break
                end
            end
        end
    end
    
    return doorNumber
end

local function addDoorESP()
    for _, door in pairs(workspace:GetDescendants()) do
        if door:IsA("Model") and door.Name == "Door" and not door:FindFirstChild("WenerESP") then
            local doorPart = door:FindFirstChild("Door") or door:FindFirstChildWhichIsA("BasePart")
            if doorPart then
                local doorNumber = getDoorNumber(door)
                local doorText = "üö™ Door " .. doorNumber
                local esp = createAdvancedESP(doorPart, Color3.fromRGB(0, 255, 0), doorText, "door")
                if esp then table.insert(doorEspObjects, esp) end
            end
        end
    end
end

local function removeDoorESP()
    for _, esp in pairs(doorEspObjects) do 
        pcall(function() 
            -- Remove selection box
            local selectionBox = esp.Adornee:FindFirstChild("SelectionBox")
            if selectionBox then selectionBox:Destroy() end
            esp:Destroy() 
        end) 
    end
    doorEspObjects = {}
end

local function enableDoorESP()
    doorEspEnabled = true
    addDoorESP()
    if doorConnection then doorConnection:Disconnect() end
    doorConnection = workspace.DescendantAdded:Connect(function(v)
        if doorEspEnabled and v:IsA("Model") and v.Name == "Door" then
            task.wait(0.5)
            addDoorESP()
        end
    end)
end

local function disableDoorESP()
    doorEspEnabled = false
    if doorConnection then doorConnection:Disconnect() end
    removeDoorESP()
end

-- ============================================================================
-- ENHANCED FIGURE ESP WITH WARNING SYSTEM
-- ============================================================================

local figureConnection

local function addFigureESP()
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("Model") and (v.Name == "FigureRagdoll" or v.Name == "Figure") and not v:FindFirstChild("WenerESP") then
            local part = v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")
            if part then
                local esp = createAdvancedESP(part, Color3.fromRGB(255, 0, 0), "üëπ FIGURE - DANGER!", "figure")
                if esp then 
                    table.insert(figureEspObjects, esp)
                    
                    -- Play warning sound
                    local sound = Instance.new("Sound")
                    sound.SoundId = "rbxassetid://131961136" -- Siren sound
                    sound.Volume = 0.8
                    sound.Looped = true
                    sound.Parent = workspace
                    sound:Play()
                    
                    -- Stop sound after 10 seconds
                    task.spawn(function()
                        wait(10)
                        sound:Stop()
                        sound:Destroy()
                    end)
                end
            end
        end
    end
end

local function removeFigureESP()
    for _, esp in pairs(figureEspObjects) do 
        pcall(function() 
            -- Remove warning light
            local light = esp.Adornee:FindFirstChild("PointLight")
            if light then light:Destroy() end
            esp:Destroy() 
        end) 
    end
    figureEspObjects = {}
end

local function enableFigureESP()
    figureEspEnabled = true
    addFigureESP()
    if figureConnection then figureConnection:Disconnect() end
    figureConnection = workspace.DescendantAdded:Connect(function(v)
        if figureEspEnabled and v:IsA("Model") and (v.Name == "FigureRagdoll" or v.Name == "Figure") then
            task.wait(0.5)
            
            addFigureESP()
        end
    end)
end

local function disableFigureESP()
    figureEspEnabled = false
    if figureConnection then figureConnection:Disconnect() end
    removeFigureESP()
end

