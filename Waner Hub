local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local GuiService = game:GetService("GuiService")
local TeleportService = game:GetService("TeleportService")
local Lighting = game:GetService("Lighting")
local UserInputService = game:GetService("UserInputService")
local SoundService = game:GetService("SoundService")
local player = Players.LocalPlayer

-- ============================================================================
-- AUTO EXECUTE SYSTEM
-- ============================================================================

local function setupAutoExecute()
    local autoExecuteCode = [[
-- WANER HUB ULTIMATE AUTO EXECUTE COMPLETE
print("üöÄ WANER HUB Auto Execute Triggered!")
loadstring(game:HttpGet("https://raw.githubusercontent.com/WanerScripts/WenerHub/main/doors-ultimate.lua"))()
]]
    
    if not isfolder("WenerHub") then
        makefolder("WenerHub")
    end
    
    writefile("WenerHub/AutoExecute.lua", autoExecuteCode)
    print("üöÄ Auto Execute: CONFIGURED SUCCESSFULLY")
end

-- ============================================================================
-- ADVANCED DISCONNECT DETECTION & AUTO RECONNECT SYSTEM
-- ============================================================================

local function setupAutoReconnect()
    local disconnectDetected = false
    
    local function createDisconnectGUI()
        local ScreenGui = Instance.new("ScreenGui")
        ScreenGui.Name = "WenerDisconnectGUI"
        ScreenGui.ResetOnSpawn = false
        ScreenGui.Parent = game.CoreGui
        
        local Blur = Instance.new("BlurEffect")
        Blur.Size = 0
        Blur.Parent = Lighting
        
        local Background = Instance.new("Frame")
        Background.Size = UDim2.new(1, 0, 1, 0)
        Background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Background.BackgroundTransparency = 1
        Background.BorderSizePixel = 0
        Background.Parent = ScreenGui
        
        local DisconnectPanel = Instance.new("Frame")
        DisconnectPanel.Size = UDim2.new(0, 650, 0, 400)
        DisconnectPanel.Position = UDim2.new(0.5, -325, 0.5, -200)
        DisconnectPanel.BackgroundColor3 = Color3.fromRGB(15, 15, 25)
        DisconnectPanel.BorderSizePixel = 0
        DisconnectPanel.Parent = ScreenGui
        
        local PanelCorner = Instance.new("UICorner")
        PanelCorner.CornerRadius = UDim.new(0, 25)
        PanelCorner.Parent = DisconnectPanel
        
        local Gradient = Instance.new("UIGradient")
        Gradient.Color = ColorSequence.new({
            ColorSequenceKeypoint.new(0, Color3.fromRGB(139, 0, 255)),
            ColorSequenceKeypoint.new(0.5, Color3.fromRGB(75, 0, 130)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(139, 0, 255))
        })
        Gradient.Rotation = 45
        Gradient.Parent = DisconnectPanel
        
        local Icon = Instance.new("TextLabel")
        Icon.Size = UDim2.new(1, 0, 0, 80)
        Icon.Position = UDim2.new(0, 0, 0, 30)
        Icon.BackgroundTransparency = 1
        Icon.Text = "‚ö†Ô∏è CONNECTION LOST"
        Icon.TextColor3 = Color3.fromRGB(255, 255, 255)
        Icon.Font = Enum.Font.GothamBold
        Icon.TextSize = 42
        Icon.TextStrokeTransparency = 0.3
        Icon.Parent = DisconnectPanel
        
        local Message = Instance.new("TextLabel")
        Message.Size = UDim2.new(1, -40, 0, 80)
        Message.Position = UDim2.new(0, 20, 0, 130)
        Message.BackgroundTransparency = 1
        Message.Text = "üîå You have been disconnected from the server\nüì° WANER HUB Ultimate Auto Reconnect System Activated\n‚ú® All features will be restored automatically"
        Message.TextColor3 = Color3.fromRGB(255, 255, 255)
        Message.Font = Enum.Font.Gotham
        Message.TextSize = 20
        Message.TextWrapped = true
        Message.TextYAlignment = Enum.TextYAlignment.Top
        Message.Parent = DisconnectPanel
        
        local AutoInfo = Instance.new("TextLabel")
        AutoInfo.Size = UDim2.new(1, -40, 0, 50)
        AutoInfo.Position = UDim2.new(0, 20, 0, 230)
        AutoInfo.BackgroundTransparency = 1
        AutoInfo.Text = "üîÑ Auto Reconnecting in 5 seconds..."
        AutoInfo.TextColor3 = Color3.fromRGB(100, 255, 100)
        AutoInfo.Font = Enum.Font.GothamBold
        AutoInfo.TextSize = 24
        AutoInfo.Parent = DisconnectPanel
        
        local ReconnectBtn = Instance.new("TextButton")
        ReconnectBtn.Size = UDim2.new(0, 280, 0, 60)
        ReconnectBtn.Position = UDim2.new(0.5, -140, 0, 300)
        ReconnectBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        ReconnectBtn.Text = "üîÑ Reconnect Now"
        ReconnectBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
        ReconnectBtn.Font = Enum.Font.GothamBold
        ReconnectBtn.TextSize = 22
        ReconnectBtn.Parent = DisconnectPanel
        
        local BtnCorner = Instance.new("UICorner")
        BtnCorner.CornerRadius = UDim.new(0, 15)
        BtnCorner.Parent = ReconnectBtn
        
        -- Enhanced Animations
        TweenService:Create(Blur, TweenInfo.new(0.8), {Size = 30}):Play()
        TweenService:Create(Background, TweenInfo.new(0.8), {BackgroundTransparency = 0.4}):Play()
        wait(0.8)
        
        local countdown = 5
        local countdownConnection
        countdownConnection = RunService.Heartbeat:Connect(function()
            countdown = countdown - (1/60)
            if countdown > 0 then
                AutoInfo.Text = string.format("üîÑ Auto Reconnecting in %.1f seconds...", countdown)
            else
                countdownConnection:Disconnect()
                pcall(function()
                    TeleportService:Teleport(game.PlaceId, player)
                end)
            end
        end)
        
        ReconnectBtn.MouseButton1Click:Connect(function()
            if countdownConnection then countdownConnection:Disconnect() end
            pcall(function()
                TeleportService:Teleport(game.PlaceId, player)
            end)
        end)
    end
    
    -- Monitor connection
    local lastHeartbeat = tick()
    RunService.Heartbeat:Connect(function()
        lastHeartbeat = tick()
    end)
    
    task.spawn(function()
        while true do
            task.wait(1)
            if tick() - lastHeartbeat > 8 and not disconnectDetected then
                disconnectDetected = true
                createDisconnectGUI()
                break
            end
        end
    end)
    
    -- Handle teleport failures
    TeleportService.TeleportInitFailed:Connect(function(player, teleportResult, errorMessage)
        if not disconnectDetected then
            disconnectDetected = true
            createDisconnectGUI()
        end
    end)
end

