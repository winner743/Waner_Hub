-- ============================================================================
-- CHARACTER CONNECTION & SPEED SETUP
-- ============================================================================

local function setupCharacter(character)
    local humanoid = character:WaitForChild("Humanoid", 5)
    if humanoid then
        humanoid.WalkSpeed = currentSpeed
        
        -- Reapply god mode if enabled
        if godModeEnabled then
            task.spawn(function()
                wait(1)
                enableGodMode()
            end)
        end
        
        -- Reapply noclip if enabled
        if noclipEnabled then
            task.spawn(function()
                wait(1)
                enableNoclip()
            end)
        end
    end
end

-- Setup for current character
if player.Character then
    setupCharacter(player.Character)
end

-- Setup for future characters
player.CharacterAdded:Connect(setupCharacter)

-- ============================================================================
-- KEY EXPIRY MONITORING & AUTO FEATURES
-- ============================================================================

if not isOwner then
    task.spawn(function()
        while true do
            wait(300) -- Check every 5 minutes
            
            local keyValid, timeLeft = checkKey()
            
            if not keyValid then
                createNotification(
                    "üîë Secret Access Expired!",
                    "Your premium access has expired.\nSecret key authentication required!\nScript will close in 10 seconds...",
                    Color3.fromRGB(255, 0, 0),
                    10
                )
                
                wait(10)
                player:Kick("üîë Secret access expired! Premium key authentication required.")
            elseif timeLeft and timeLeft < 1800 then -- 30 minutes warning
                createNotification(
                    "‚ö†Ô∏è Secret Access Expiring!",
                    "üîê Secret access expires in: " .. formatTime(timeLeft) .. "\n‚è∞ Prepare for re-authentication!",
                    Color3.fromRGB(255, 165, 0),
                    8
                )
            elseif timeLeft and timeLeft < 3600 then -- 1 hour warning
                createNotification(
                    "‚è∞ 1 Hour Remaining",
                    "Secret access expires in: " .. formatTime(timeLeft),
                    Color3.fromRGB(255, 200, 0),
                    5
                )
            end
        end
    end)
end

-- ============================================================================
-- CLEANUP ON LEAVING
-- ============================================================================

game:GetService("Players").PlayerRemoving:Connect(function(leavingPlayer)
    if leavingPlayer == player then
        -- Cleanup all connections
        for _, conn in pairs(entityConnections) do
            if conn then conn:Disconnect() end
        end
        
        for _, conn in pairs(godModeConnections) do
            if conn then conn:Disconnect() end
        end
        
        for _, conn in pairs(bypassConnections) do
            if conn then conn:Disconnect() end
        end
        
        if noclipConnection then noclipConnection:Disconnect() end
        if autoCollectConnection then autoCollectConnection:Disconnect() end
        if autoUnlockConnection then autoUnlockConnection:Disconnect() end
        if autoProximityPromptConnection then autoProximityPromptConnection:Disconnect() end
        if infVitaminsConnection then infVitaminsConnection:Disconnect() end
        
        -- Clear ESP
        for _, esp in pairs(espObjects) do
            if esp.highlight then esp.highlight:Destroy() end
            if esp.billboard then esp.billboard:Destroy() end
        end
        
        print("üßπ WENER HUB: Cleanup completed")
    end
end)

-- ============================================================================
-- ANTI-DETECTION & STEALTH MODE
-- ============================================================================

-- Hide script traces
local function hideScript()
    -- Remove from global namespace
    getgenv().WenerHub = nil
    _G.WenerHub = nil
    shared.WenerHub = nil
    
    -- Clear debug info
    pcall(function()
        debug.traceback = function() return "" end
    end)
end

task.spawn(function()
    wait(5)
    hideScript()
end)

