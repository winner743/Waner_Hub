        Callback = function()
            local currentRooms = workspace:FindFirstChild("CurrentRooms")
            if currentRooms and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local highestRoom = 0
                local targetRoom = nil
                
                for _, room in pairs(currentRooms:GetChildren()) do
                    local roomNum = tonumber(room.Name)
                    if roomNum and roomNum > highestRoom then
                        highestRoom = roomNum
                        targetRoom = room
                    end
                end
                
                if targetRoom then
                    local roomPart = targetRoom:FindFirstChildWhichIsA("BasePart")
                    if roomPart then
                        player.Character.HumanoidRootPart.CFrame = roomPart.CFrame + Vector3.new(0, 5, 0)
                        createNotification("üéØ Latest Room", "Teleported to Room " .. highestRoom .. " (Latest)", Color3.fromRGB(0, 255, 100), 4)
                    end
                end
            end
        end
    })
    
