        disableDoorESP()
        disableBookESP()
        disableKeyESP() 
        disableFigureESP()
        disableEntityESP()
        disablePlayerESP()
        disableItemESP()
        
        -- Clean up any leftover objects
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj.Name == "WenerESP" then
                obj:Destroy()
            end
        end
        
        Rayfield:Notify({
            Title = "üóëÔ∏è Workspace Cleaned",
            Content = "‚úÖ All ESP objects removed" ..
                     "\nüßπ Workspace cleaned up",
            Duration = 3,
            Image = 4483362458,
        })
    end
})

