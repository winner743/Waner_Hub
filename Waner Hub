    if bookConnection then bookConnection:Disconnect() end
    removeBookESP()
end

-- ============================================================================
-- ENHANCED KEY ESP
-- ============================================================================

local keyConnection

local function addKeyESP()
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("Model") and (v.Name == "Key" or v.Name == "SkeletonKey") and not v:FindFirstChild("WenerESP") then
            local part = v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")
            if part then
                local keyType = v.Name == "SkeletonKey" and "üóùÔ∏è Skeleton Key" or "üîë Key"
                local color = v.Name == "SkeletonKey" and Color3.fromRGB(255, 215, 0) or Color3.fromRGB(255, 255, 0)
                local esp = createAdvancedESP(part, color, keyType, "key")
                if esp then table.insert(keyEspObjects, esp) end
            end
        end
    end
end

local function removeKeyESP()
    for _, esp in pairs(keyEspObjects) do pcall(function() esp:Destroy() end) end
    keyEspObjects = {}
end

local function enableKeyESP()
    keyEspEnabled = true
    addKeyESP()
    if keyConnection then keyConnection:Disconnect() end
    keyConnection = workspace.DescendantAdded:Connect(function(v)
        if keyEspEnabled and v:IsA("Model") and (v.Name == "Key" or v.Name == "SkeletonKey") then
            task.wait(0.5)
            addKeyESP()
        end
    end)
end

local function disableKeyESP()
    keyEspEnabled = false
    if keyConnection then keyConnection:Disconnect() end
    removeKeyESP()
end

-- ============================================================================
-- ENHANCED ENTITY ESP
-- ============================================================================

local entityConnection
local entityList = {
    "RushMoving", "AmbushMoving", "Eyes", "Screech", "Halt", "Seek", 
    "Figure", "BackdoorLookman", "A60", "A90", "Timothy", "Snare",
    "Dupe", "Hide", "Glitch", "Jack", "Shadow"
}

local function addEntityESP()
    for _, entityName in pairs(entityList) do
        for _, v in pairs(workspace:GetDescendants()) do
            if v:IsA("Model") and v.Name:find(entityName) and not v:FindFirstChild("WenerESP") then
                local part = v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")
                if part then
                    local color = Color3.fromRGB(255, 0, 0)
                    if entityName == "Eyes" then color = Color3.fromRGB(0, 255, 255) end
                    if entityName == "Halt" then color = Color3.fromRGB(255, 255, 0) end
                    if entityName == "Seek" then color = Color3.fromRGB(0, 0, 0) end
                    
                    local esp = createAdvancedESP(part, color, "üëª " .. entityName:gsub("Moving", ""), "entity")
                    if esp then table.insert(entityEspObjects, esp) end
                end
            end
        end
    end
end

local function removeEntityESP()
    for _, esp in pairs(entityEspObjects) do pcall(function() esp:Destroy() end) end
    entityEspObjects = {}
end

local function enableEntityESP()
    entityEspEnabled = true
    addEntityESP()
    if entityConnection then entityConnection:Disconnect() end
    entityConnection = workspace.DescendantAdded:Connect(function(v)
        if entityEspEnabled then
            for _, entityName in pairs(entityList) do
                if v:IsA("Model") and v.Name:find(entityName) then
                    task.wait(0.5)
                    addEntityESP()
                    break
                end
            end
        end
    end)
end

local function disableEntityESP()
    entityEspEnabled = false
    if entityConnection then entityConnection:Disconnect() end
    removeEntityESP()
end

-- ============================================================================
-- ENHANCED PLAYER ESP
-- ============================================================================

local playerConnection

local function addPlayerESP()
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local part = otherPlayer.Character.HumanoidRootPart
            if not part:FindFirstChild("WenerESP") then
                local esp = createAdvancedESP(part, Color3.fromRGB(0, 255, 255), "üë§ " .. otherPlayer.Name, "player")
                if esp then table.insert(playerEspObjects, esp) end
            end
        end
    end
end

local function removePlayerESP()
    for _, esp in pairs(playerEspObjects) do pcall(function() esp:Destroy() end) end
    playerEspObjects = {}
end

local function enablePlayerESP()
    playerEspEnabled = true
    addPlayerESP()
    if playerConnection then playerConnection:Disconnect() end
    playerConnection = Players.PlayerAdded:Connect(function()
        if playerEspEnabled then
            task.wait(2)
            addPlayerESP()
        end
    end)
end

local function disablePlayerESP()
    playerEspEnabled = false
    if playerConnection then playerConnection:Disconnect() end
    removePlayerESP()
end

-- ============================================================================
-- ENHANCED ITEM ESP
-- ============================================================================

local itemConnection
local itemList = {
    "Vitamins", "Lockpick", "FlashlightPickup", "Lighter", "Candle", 
    "Crucifix", "Bandage", "Shakelight", "Scanner", "Paper"
}

local function addItemESP()
    for _, itemName in pairs(itemList) do
        for _, v in pairs(workspace:GetDescendants()) do
            if v:IsA("Model") and v.Name == itemName and not v:FindFirstChild("WenerESP") then
                local part = v.PrimaryPart or v:FindFirstChildWhichIsA("BasePart")
                if part then
                    local color = Color3.fromRGB(0, 255, 0)
                    if itemName == "Crucifix" then color = Color3.fromRGB(255, 215, 0) end
                    if itemName == "Vitamins" then color = Color3.fromRGB(255, 0, 255) end
                    
                    local esp = createAdvancedESP(part, color, "üì¶ " .. itemName, "item")
                    if esp then table.insert(itemEspObjects, esp) end
                end
            end
        end
    end
end

local function removeItemESP()
    for _, esp in pairs(itemEspObjects) do pcall(function() esp:Destroy() end) end
    itemEspObjects = {}
end

local function enableItemESP()
    itemEspEnabled = true
    addItemESP()
    if itemConnection then itemConnection:Disconnect() end
    itemConnection = workspace.DescendantAdded:Connect(function(v)
        if itemEspEnabled then
            for _, itemName in pairs(itemList) do
                if v:IsA("Model") and v.Name == itemName then
                    task.wait(0.5)
                    addItemESP()
                    break
                end
            end
        end
    end)
end

local function disableItemESP()
    itemEspEnabled = false
    if itemConnection then itemConnection:Disconnect() end
    removeItemESP()
end

-- ============================================================================
-- ENHANCED ENTITY NOTIFICATIONS WITH SOUND
-- ============================================================================

local function setupEntityNotifications()
    workspace.ChildAdded:Connect(function(child)
        if rushNotifEnabled and (child.Name == "RushMoving" or child.Name == "AmbushMoving") then
            local name = child.Name:gsub("Moving", ""):upper()
            
            -- Play warning sound
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://4590662766" -- Emergency sound
            sound.Volume = 1
            sound.Parent = workspace
            sound:Play()
            sound.Ended:Connect(function() sound:Destroy() end)
            
            Rayfield:Notify({
                Title = "‚ö†Ô∏è " .. name .. " SPAWNED!",
                Content = "üö® HIDE IMMEDIATELY! " .. name .. " is approaching!\nüí° Find a closet or bed to hide!",
                Duration = 8,
                Image = 4483362458,
            })
            
            -- Screen flash effect
            local flash = Instance.new("ScreenGui")
            flash.Name = "WenerFlash"
            flash.Parent = game.CoreGui
            
            local frame = Instance.new("Frame")
            frame.Size = UDim2.new(1, 0, 1, 0)
            frame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            frame.BackgroundTransparency = 0.5
            frame.Parent = flash
            
            TweenService:Create(frame, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
            task.wait(0.2)
            flash:Destroy()
        end
        
        if allEntityNotifications then
            for _, entityName in pairs(entityList) do
                if child.Name:find(entityName) then
                    local color = "üëª"
                    if entityName == "Eyes" then color = "üëÅÔ∏è" end
                    if entityName == "Screech" then color = "ü¶á" end
                    if entityName == "Halt" then color = "‚úã" end
                    if entityName == "Seek" then color = "üñ§" end
                    if entityName == "Figure" then color = "üëπ" end
                    
                    Rayfield:Notify({
                        Title = color .. " ENTITY DETECTED!",
                        Content = "Entity: " .. entityName .. "\nüõ°Ô∏è Stay alert and be careful!",
                        Duration = 5,
                        Image = 4483362458,
                    })
                    break
                end
            end
        end
    end)
end

-- ============================================================================
-- ENHANCED INFINITE VITAMINS
-- ============================================================================

local infVitaminsConnection

local function enableInfVitamins()
    infVitaminsEnabled = true
    
    if infVitaminsConnection then infVitaminsConnection:Disconnect() end
    
    infVitaminsConnection = RunService.Heartbeat:Connect(function()
        if infVitaminsEnabled and player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid and humanoid.Health < humanoid.MaxHealth then
                humanoid.Health = humanoid.MaxHealth
            end
        end
    end)
    
    print("üíä Infinite Vitamins: ENABLED")
end

local function disableInfVitamins()
    infVitaminsEnabled = false
    if infVitaminsConnection then infVitaminsConnection:Disconnect() end
    print("üíä Infinite Vitamins: DISABLED")
end

-- ============================================================================
-- ENHANCED AUTO UNLOCK DOORS
-- ============================================================================

local autoUnlockConnection

local function enableAutoUnlock()
    autoUnlockEnabled = true
    
    if autoUnlockConnection then autoUnlockConnection:Disconnect() end
    
    local function unlockDoors()
        for _, door in pairs(workspace:GetDescendants()) do
            if door:IsA("Model") and door.Name == "Door" then
                local prompt = door:FindFirstChildOfClass("ProximityPrompt", true)
                if prompt and prompt.Enabled and prompt.ActionText == "Open" then
                    local doorPart = door:FindFirstChildWhichIsA("BasePart", true)
                    if doorPart and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        local distance = (player.Character.HumanoidRootPart.Position - doorPart.Position).Magnitude
                        if distance <= 15 then
                            pcall(function()
                                fireproximityprompt(prompt)
                            end)
                        end
                    end
                end
            end
        end
    end
    
    autoUnlockConnection = RunService.Heartbeat:Connect(function()
        if autoUnlockEnabled then
            unlockDoors()
        end
    end)
    
    print("üîì Auto Unlock: ENABLED")
end

local function disableAutoUnlock()
    autoUnlockEnabled = false
    if autoUnlockConnection then autoUnlockConnection:Disconnect() end
    print("üîì Auto Unlock: DISABLED")
end

