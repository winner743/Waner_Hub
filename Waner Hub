local espObjects = {}
local keyEspObjects = {}
local entityEspObjects = {}
local playerEspObjects = {}
local bookEspObjects = {}
local itemEspObjects = {}
local figureEspObjects = {}
local doorEspObjects = {}

-- States
local doorEspEnabled = false
local keyEspEnabled = false
local entityEspEnabled = false
local playerEspEnabled = false
local figureEspEnabled = false
local bookEspEnabled = false
local itemEspEnabled = false
local rushNotifEnabled = false
local allEntityNotifications = false
local infVitaminsEnabled = false
local autoUnlockEnabled = false
local autoCollectEnabled = false
local noclipEnabled = false
local godModeEnabled = false
local bypassAntiCheat = false
local currentSpeed = 1

-- ============================================================================
-- ADVANCED GOD MODE SYSTEM WITH MULTIPLE PROTECTION LAYERS
-- ============================================================================

local godModeConnections = {}
local originalHealth = nil

local function enableGodMode()
    godModeEnabled = true
    
    for _, conn in pairs(godModeConnections) do
        pcall(function() conn:Disconnect() end)
    end
    godModeConnections = {}
    
    local function setupGodMode(character)
        if not character then return end
        
        local humanoid = character:WaitForChild("Humanoid", 5)
        if not humanoid then return end
        
        if not originalHealth then
            originalHealth = humanoid.MaxHealth
        end
        
        -- Multiple protection layers
        humanoid.MaxHealth = math.huge
        humanoid.Health = math.huge
        
        -- Block health changes
        local healthConn = humanoid:GetPropertyChangedSignal("Health"):Connect(function()
            if godModeEnabled and humanoid.Health < math.huge then
                humanoid.Health = math.huge
            end
        end)
        
        local maxHealthConn = humanoid:GetPropertyChangedSignal("MaxHealth"):Connect(function()
            if godModeEnabled and humanoid.MaxHealth < math.huge then
                humanoid.MaxHealth = math.huge
                humanoid.Health = math.huge
            end
        end)
        
        -- Block damage events
        local damageConn = humanoid.HealthChanged:Connect(function(health)
            if godModeEnabled and health < humanoid.MaxHealth then
                humanoid.Health = math.huge
            end
        end)
        
        table.insert(godModeConnections, healthConn)
        table.insert(godModeConnections, maxHealthConn)
        table.insert(godModeConnections, damageConn)
    end
    
    if player.Character then
        setupGodMode(player.Character)
    end
    
    local charConn = player.CharacterAdded:Connect(function(character)
        if godModeEnabled then
            task.wait(1)
            setupGodMode(character)
        end
    end)
    
    table.insert(godModeConnections, charConn)
    print("üõ°Ô∏è Advanced God Mode: ENABLED")
end

local function disableGodMode()
    godModeEnabled = false
    
    for _, conn in pairs(godModeConnections) do
        pcall(function() conn:Disconnect() end)
    end
    godModeConnections = {}
    
    if player.Character then
        local humanoid = player.Character:FindFirstChild("Humanoid")
        if humanoid and originalHealth then
            humanoid.MaxHealth = originalHealth
            humanoid.Health = originalHealth
        end
    end
    
    print("üõ°Ô∏è Advanced God Mode: DISABLED")
end

-- ============================================================================
-- ADVANCED BYPASS ANTI-CHEAT SYSTEM
-- ============================================================================

local bypassConnections = {}

local function enableBypassAntiCheat()
    bypassAntiCheat = true
    
    for _, conn in pairs(bypassConnections) do
        pcall(function() conn:Disconnect() end)
    end
    bypassConnections = {}
    
    local function setupBypass(character)
        if not character then return end
        
        local humanoid = character:WaitForChild("Humanoid", 5)
        if not humanoid then return end
        
        -- Block anti-cheat remotes
        local function blockRemotes()
            for _, remote in pairs(ReplicatedStorage:GetDescendants()) do
                if remote:IsA("RemoteEvent") or remote:IsA("RemoteFunction") then
                    local name = remote.Name:lower()
                    if name:find("anticheat") or name:find("speed") or name:find("hack") or name:find("exploit") then
                        local oldFireServer = remote.FireServer
                        if oldFireServer then
                            remote.FireServer = function(...)
                                if bypassAntiCheat then
                                    return
                                end
                                return oldFireServer(...)
                            end
                        end
                        
                        local oldInvokeServer = remote.InvokeServer
                        if oldInvokeServer then
                            remote.InvokeServer = function(...)
                                if bypassAntiCheat then
                                    return
                                end
                                return oldInvokeServer(...)
                            end
                        end
                    end
                end
            end
        end
        
        blockRemotes()
        
        local remoteConn = ReplicatedStorage.DescendantAdded:Connect(function(descendant)
            if bypassAntiCheat then
                task.wait(0.1)
                blockRemotes()
            end
        end)
        
        table.insert(bypassConnections, remoteConn)
    end
    
    if player.Character then
        setupBypass(player.Character)
    end
    
    local charConn = player.CharacterAdded:Connect(function(character)
        if bypassAntiCheat then
            task.wait(1)
            setupBypass(character)
        end
    end)
    
    table.insert(bypassConnections, charConn)
    print("üîì Advanced Bypass Anti-Cheat: ENABLED")
end

local function disableBypassAntiCheat()
    bypassAntiCheat = false
    
    for _, conn in pairs(bypassConnections) do
        pcall(function() conn:Disconnect() end)
    end
    bypassConnections = {}
    
    print("üîì Advanced Bypass Anti-Cheat: DISABLED")
end

-- ============================================================================
-- ADVANCED SPEED SYSTEM WITH SMOOTH TRANSITIONS
-- ============================================================================

local function setSpeed(multiplier)
    currentSpeed = multiplier
    
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        local humanoid = player.Character.Humanoid
        local targetSpeed = 16 * multiplier
        
        if bypassAntiCheat then
            -- Gradual speed change to avoid detection
            local steps = math.max(1, math.abs(targetSpeed - humanoid.WalkSpeed) / 3)
            for i = 1, steps do
                if not player.Character or not player.Character:FindFirstChild("Humanoid") then break end
                
                local progress = i / steps
                local newSpeed = humanoid.WalkSpeed + (targetSpeed - humanoid.WalkSpeed) * progress
                
                humanoid.WalkSpeed = newSpeed
                task.wait(0.05)
            end
        else
            humanoid.WalkSpeed = targetSpeed
        end
    end
end

-- Apply speed on character spawn
player.CharacterAdded:Connect(function(character)
    task.wait(1)
    if character:FindFirstChild("Humanoid") then
        setSpeed(currentSpeed)
    end
end)

-- ============================================================================
-- ADVANCED NOCLIP SYSTEM
-- ============================================================================

local noclipConnection
local noclipParts = {}

local function enableNoclip()
    noclipEnabled = true
    
    if noclipConnection then noclipConnection:Disconnect() end
    
    local function applyNoclip()
        if not player.Character then return end
        
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide then
                part.CanCollide = false
                noclipParts[part] = true
            end
        end
    end
    
    applyNoclip()
    
    noclipConnection = RunService.Stepped:Connect(function()
        if noclipEnabled then
            applyNoclip()
        end
    end)
    
    print("üëª Advanced Noclip: ENABLED")
end

local function disableNoclip()
    noclipEnabled = false
    
    if noclipConnection then noclipConnection:Disconnect() end
    
    for part, _ in pairs(noclipParts) do
        if part and part.Parent then
            part.CanCollide = true
        end
    end
    noclipParts = {}
    
    print("üëª Advanced Noclip: DISABLED")
end

-- ============================================================================
-- ADVANCED AUTO COLLECT SYSTEM WITH SMART DETECTION
-- ============================================================================

local autoCollectConnection
local autoCollectSettings = {
    Vitamins = true,
    Lockpick = true,
    FlashlightPickup = true,
    Lighter = true,
    Candle = true,
    Crucifix = true,
    Bandage = true,
    SkeletonKey = true,
    Shakelight = true,
    Key = true,
    Book = true,
    Paper = true,
    Scanner = true,
    ElectricalKeyCard = true
}

local function autoCollectItems()
    if not autoCollectEnabled then return end
    
    local collectedCount = 0
    
    local function collectFromContainer(container)
        for _, item in pairs(container:GetDescendants()) do
            if collectedCount >= 20 then break end
            
            for itemName, enabled in pairs(autoCollectSettings) do
                if enabled and item:IsA("Model") and (item.Name == itemName or item.Name:find(itemName)) then
                    local prompt = item:FindFirstChildOfClass("ProximityPrompt", true)
                    if not prompt then
                        for _, descendant in pairs(item:GetDescendants()) do
                            if descendant:IsA("ProximityPrompt") and descendant.Enabled then
                                prompt = descendant
                                break
                            end
                        end
                    end
                    
                    if prompt and prompt.Enabled then
                        local itemPart = item:FindFirstChildWhichIsA("BasePart", true)
                        if itemPart and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            local distance = (player.Character.HumanoidRootPart.Position - itemPart.Position).Magnitude
                            
                            if distance <= (prompt.MaxActivationDistance or 15) then
                                pcall(function()
                                    fireproximityprompt(prompt)
                                    collectedCount = collectedCount + 1
                                end)
                                task.wait(0.1)
                            end
                        end
                    end
                end
            end
        end
    end
    
    collectFromContainer(workspace)
    
    local currentRooms = workspace:FindFirstChild("CurrentRooms")
    if currentRooms then
        collectFromContainer(currentRooms)
    end
end

local function enableAutoCollect()
    autoCollectEnabled = true
    
    if autoCollectConnection then autoCollectConnection:Disconnect() end
    
    autoCollectConnection = RunService.Heartbeat:Connect(function()
        if autoCollectEnabled then
            autoCollectItems()
        end
    end)
    
    print("üì¶ Advanced Auto Collect: ENABLED")
end

local function disableAutoCollect()
    autoCollectEnabled = false
    
    if autoCollectConnection then autoCollectConnection:Disconnect() end
    
    print("üì¶ Advanced Auto Collect: DISABLED")
end

-- ============================================================================
-- ENHANCED ESP SYSTEM WITH ADVANCED FEATURES & LIGHT EFFECTS
-- ============================================================================

-- Create enhanced ESP with light effect and animations
local function createAdvancedESP(obj, color, txt, espType)
    if not obj or obj:FindFirstChild("WenerESP") then return nil end
    
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "WenerESP"
    billboard.Adornee = obj
    billboard.Size = UDim2.new(0, 180, 0, 90)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true
    billboard.Parent = obj
    
    -- Background frame with gradient
    local bgFrame = Instance.new("Frame")
    bgFrame.Size = UDim2.new(1, 0, 1, 0)
    bgFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    bgFrame.BackgroundTransparency = 0.2
    bgFrame.BorderSizePixel = 0
    bgFrame.Parent = billboard
    
    local bgCorner = Instance.new("UICorner")
    bgCorner.    bgCorner.CornerRadius = UDim.new(0, 8)
    bgCorner.Parent = bgFrame
    
    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.new(color.R * 0.3, color.G * 0.3, color.B * 0.3)),
        ColorSequenceKeypoint.new(1, Color3.new(color.R * 0.1, color.G * 0.1, color.B * 0.1))
    })
    gradient.Rotation = 45
    gradient.Parent = bgFrame
    
    -- Main text label
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, -10, 0.6, 0)
    label.Position = UDim2.new(0, 5, 0, 5)
    label.BackgroundTransparency = 1
    label.Text = txt
    label.TextColor3 = color
    label.TextStrokeTransparency = 0
    label.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
    label.TextScaled = true
    label.Font = Enum.Font.GothamBold
    label.Parent = billboard
    
    -- Distance label
    local distanceLabel = Instance.new("TextLabel")
    distanceLabel.Size = UDim2.new(1, -10, 0.4, 0)
    distanceLabel.Position = UDim2.new(0, 5, 0.6, 0)
    distanceLabel.BackgroundTransparency = 1
    distanceLabel.Text = "0m"
    distanceLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    distanceLabel.TextStrokeTransparency = 0
    distanceLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
    distanceLabel.TextScaled = true
    distanceLabel.Font = Enum.Font.Gotham
    distanceLabel.Parent = billboard
    
    -- Light effect
    local lightEffect = Instance.new("PointLight")
    lightEffect.Color = color
    lightEffect.Brightness = 2
    lightEffect.Range = 10
    lightEffect.Parent = obj
    
    -- Pulsing animation
    task.spawn(function()
        while billboard.Parent do
            TweenService:Create(lightEffect, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
                Brightness = 0.5
            }):Play()
            
            TweenService:Create(label, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
                TextTransparency = 0.3
            }):Play()
            
            task.wait(1)
        end
    end)
    
    -- Distance updater
    task.spawn(function()
        while billboard.Parent and player.Character and player.Character:FindFirstChild("HumanoidRootPart") do
            local distance = math.floor((player.Character.HumanoidRootPart.Position - obj.Position).Magnitude)
            distanceLabel.Text = distance .. "m"
            task.wait(0.5)
        end
    end)
    
    return billboard
end

-- Door ESP System
local function updateDoorESP()
    for _, esp in pairs(doorEspObjects) do
        if esp then esp:Destroy() end
    end
    doorEspObjects = {}
    
    if not doorEspEnabled then return end
    
    local function addDoorESP(door)
        if door:FindFirstChild("WenerESP") then return end
        
        local doorNumber = door.Name
        local esp = createAdvancedESP(door:FindFirstChildWhichIsA("BasePart"), Color3.fromRGB(0, 255, 0), "üö™ Door " .. doorNumber, "door")
        if esp then
            table.insert(doorEspObjects, esp)
        end
    end
    
    -- Current rooms doors
    local currentRooms = workspace:FindFirstChild("CurrentRooms")
    if currentRooms then
        for _, room in pairs(currentRooms:GetChildren()) do
            if room:IsA("Model") and tonumber(room.Name) then
                local door = room:FindFirstChild("Door")
                if door then
                    addDoorESP(door)
                end
            end
        end
    end
    
    -- Monitor new doors
    if currentRooms then
        currentRooms.ChildAdded:Connect(function(room)
            if doorEspEnabled and room:IsA("Model") and tonumber(room.Name) then
                task.wait(1)
                local door = room:FindFirstChild("Door")
                if door then
                    addDoorESP(door)
                end
            end
        end)
    end
end

-- Key ESP System
local function updateKeyESP()
    for _, esp in pairs(keyEspObjects) do
        if esp then esp:Destroy() end
    end
    keyEspObjects = {}
    
    if not keyEspEnabled then return end
    
    local function addKeyESP(key)
        if key:FindFirstChild("WenerESP") then return end
        
        local esp = createAdvancedESP(key:FindFirstChildWhichIsA("BasePart"), Color3.fromRGB(255, 255, 0), "üóùÔ∏è Key", "key")
        if esp then
            table.insert(keyEspObjects, esp)
        end
    end
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("Model") and obj.Name == "Key" then
            addKeyESP(obj)
        end
    end
    
    workspace.DescendantAdded:Connect(function(obj)
        if keyEspEnabled and obj:IsA("Model") and obj.Name == "Key" then
            task.wait(0.5)
            addKeyESP(obj)
        end
    end)
end

-- Entity ESP System
local function updateEntityESP()
    for _, esp in pairs(entityEspObjects) do
        if esp then esp:Destroy() end
    end
    entityEspObjects = {}
    
    if not entityEspEnabled then return end
    
    local entities = {"RushMoving", "AmbushMoving", "Eyes", "Screech", "Halt", "Seek", "Figure", "Dupe", "A60", "A120"}
    
    local function addEntityESP(entity)
        if entity:FindFirstChild("WenerESP") then return end
        
        local color = Color3.fromRGB(255, 0, 0)
        local name = entity.Name
        
        if name == "Eyes" then color = Color3.fromRGB(255, 255, 255)
        elseif name == "Halt" then color = Color3.fromRGB(0, 0, 255)
        elseif name == "Seek" then color = Color3.fromRGB(0, 0, 0)
        elseif name == "Figure" then color = Color3.fromRGB(139, 0, 255)
        end
        
        local esp = createAdvancedESP(entity:FindFirstChildWhichIsA("BasePart"), color, "üëπ " .. name, "entity")
        if esp then
            table.insert(entityEspObjects, esp)
        end
    end
    
    for _, entity in pairs(workspace:GetDescendants()) do
        if entity:IsA("Model") then
            for _, entityName in pairs(entities) do
                if entity.Name == entityName or entity.Name:find(entityName) then
                    addEntityESP(entity)
                    break
                end
            end
        end
    end
    
    workspace.DescendantAdded:Connect(function(entity)
        if entityEspEnabled and entity:IsA("Model") then
            for _, entityName in pairs(entities) do
                if entity.Name == entityName or entity.Name:find(entityName) then
                    task.wait(0.5)
                    addEntityESP(entity)
                    break
                end
            end
        end
    end)
end

-- Player ESP System
local function updatePlayerESP()
    for _, esp in pairs(playerEspObjects) do
        if esp then esp:Destroy() end
    end
    playerEspObjects = {}
    
    if not playerEspEnabled then return end
    
    local function addPlayerESP(otherPlayer)
        if otherPlayer == player or not otherPlayer.Character then return end
        
        local character = otherPlayer.Character
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        
        if humanoidRootPart and not humanoidRootPart:FindFirstChild("WenerESP") then
            local esp = createAdvancedESP(humanoidRootPart, Color3.fromRGB(0, 255, 255), "üë§ " .. otherPlayer.DisplayName, "player")
            if esp then
                table.insert(playerEspObjects, esp)
            end
        end
    end
    
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            addPlayerESP(otherPlayer)
        end
    end
    
    Players.PlayerAdded:Connect(function(otherPlayer)
        if playerEspEnabled then
            otherPlayer.CharacterAdded:Connect(function()
                task.wait(1)
                addPlayerESP(otherPlayer)
            end)
        end
    end)
end

-- Book ESP System
local function updateBookESP()
    for _, esp in pairs(bookEspObjects) do
        if esp then esp:Destroy() end
    end
    bookEspObjects = {}
    
    if not bookEspEnabled then return end
    
    local function addBookESP(book)
        if book:FindFirstChild("WenerESP") then return end
        
        local esp = createAdvancedESP(book:FindFirstChildWhichIsA("BasePart"), Color3.fromRGB(139, 69, 19), "üìö Book", "book")
        if esp then
            table.insert(bookEspObjects, esp)
        end
    end
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("Model") and (obj.Name == "Book" or obj.Name:find("Book")) then
            addBookESP(obj)
        end
    end
    
    workspace.DescendantAdded:Connect(function(obj)
        if bookEspEnabled and obj:IsA("Model") and (obj.Name == "Book" or obj.Name:find("Book")) then
            task.wait(0.5)
            addBookESP(obj)
        end
    end)
end

-- Item ESP System
local function updateItemESP()
    for _, esp in pairs(itemEspObjects) do
        if esp then esp:Destroy() end
    end
    itemEspObjects = {}
    
    if not itemEspEnabled then return end
    
    local items = {
        "Vitamins", "Lockpick", "FlashlightPickup", "Lighter", "Candle", 
        "Crucifix", "Bandage", "SkeletonKey", "Shakelight", "Scanner"
    }
    
    local function addItemESP(item)
        if item:FindFirstChild("WenerESP") then return end
        
        local color = Color3.fromRGB(255, 165, 0)
        local icon = "üì¶"
        
        if item.Name == "Vitamins" then 
            color = Color3.fromRGB(255, 0, 255)
            icon = "üíä"
        elseif item.Name == "Lockpick" then 
            color = Color3.fromRGB(192, 192, 192)
            icon = "üîì"
        elseif item.Name:find("Flashlight") then 
            color = Color3.fromRGB(255, 255, 0)
            icon = "üî¶"
        elseif item.Name == "Crucifix" then 
            color = Color3.fromRGB(255, 215, 0)
            icon = "‚úùÔ∏è"
        end
        
        local esp = createAdvancedESP(item:FindFirstChildWhichIsA("BasePart"), color, icon .. " " .. item.Name, "item")
        if esp then
            table.insert(itemEspObjects, esp)
        end
    end
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("Model") then
            for _, itemName in pairs(items) do
                if obj.Name == itemName or obj.Name:find(itemName) then
                    addItemESP(obj)
                    break
                end
            end
        end
    end
    
    workspace.DescendantAdded:Connect(function(obj)
        if itemEspEnabled and obj:IsA("Model") then
            for _, itemName in pairs(items) do
                if obj.Name == itemName or obj.Name:find(itemName) then
                    task.wait(0.5)
                    addItemESP(obj)
                    break
                end
            end
        end
    end)
end

-- ============================================================================
-- ADVANCED NOTIFICATION SYSTEM WITH SOUND & VISUAL EFFECTS
-- ============================================================================

local function createAdvancedNotification(title, message, color, duration, sound)
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "WenerNotification"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.Parent = game.CoreGui
    
    local NotificationFrame = Instance.new("Frame")
    NotificationFrame.Size = UDim2.new(0, 400, 0, 120)
    NotificationFrame.Position = UDim2.new(1, 20, 0, 100)
    NotificationFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 25)
    NotificationFrame.BorderSizePixel = 0
    NotificationFrame.Parent = ScreenGui
    
    local Corner = Instance.new("UICorner")
    Corner.CornerRadius = UDim.new(0, 15)
    Corner.Parent = NotificationFrame
    
    local Gradient = Instance.new("UIGradient")
    Gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, color),
        ColorSequenceKeypoint.new(1, Color3.new(color.R * 0.5, color.G * 0.5, color.B * 0.5))
    })
    Gradient.Rotation = 45
    Gradient.Parent = NotificationFrame
    
    local TitleLabel = Instance.new("TextLabel")
    TitleLabel.Size = UDim2.new(1, -20, 0, 40)
    TitleLabel.Position = UDim2.new(0, 10, 0, 10)
    TitleLabel.BackgroundTransparency = 1
    TitleLabel.Text = title
    TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TitleLabel.Font = Enum.Font.GothamBold
    TitleLabel.TextSize = 20
    TitleLabel.TextXAlignment = Enum.TextXAlignment.Left
    TitleLabel.Parent = NotificationFrame
    
    local MessageLabel = Instance.new("TextLabel")
    MessageLabel.Size = UDim2.new(1, -20, 0, 60)
    MessageLabel.Position = UDim2.new(0, 10, 0, 50)
    MessageLabel.BackgroundTransparency = 1
    MessageLabel.Text = message
    MessageLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    MessageLabel.Font = Enum.Font.Gotham
    MessageLabel.TextSize = 16
    MessageLabel.TextWrapped = true
    MessageLabel.TextXAlignment = Enum.TextXAlignment.Left
    MessageLabel.TextYAlignment = Enum.TextYAlignment.Top
    MessageLabel.Parent = NotificationFrame
    
    -- Sound effect
    if sound then
        local soundEffect = Instance.new("Sound")
        soundEffect.SoundId = "rbxassetid://131961136"
        soundEffect.Volume = 0.5
        soundEffect.Parent = ScreenGui
        soundEffect:Play()
        
        soundEffect.Ended:Connect(function()
            soundEffect:Destroy()
        end)
    end
    
    -- Slide in animation
    TweenService:Create(NotificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {
        Position = UDim2.new(1, -420, 0, 100)
    }):Play()
    
    -- Auto dismiss
    task.spawn(function()
        task.wait(duration or 5)
        
        TweenService:Create(NotificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quad), {
            Position = UDim2.new(1, 20, 0, 100),
            BackgroundTransparency = 1
        }):Play()
        
        TweenService:Create(TitleLabel, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
        TweenService:Create(MessageLabel, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
        
        task.wait(0.5)
        ScreenGui:Destroy()
    end)
end

-- ============================================================================
-- ADVANCED ENTITY DETECTION & NOTIFICATION SYSTEM
-- ============================================================================

local entityConnections = {}
local lastEntityNotification = {}

local function setupEntityNotifications()
    for _, conn in pairs(entityConnections) do
        pcall(function() conn:Disconnect() end)
    end
    entityConnections = {}
    
    if not rushNotifEnabled and not allEntityNotifications then return end
    
    local entities = {
        {name = "RushMoving", displayName = "Rush", color = Color3.fromRGB(255, 0, 0), message = "Rush is coming! Hide immediately!"},
        {name = "AmbushMoving", displayName = "Ambush", color = Color3.fromRGB(255, 100, 0), message = "Ambush detected! Get ready to hide multiple times!"},
        {name = "Eyes", displayName = "Eyes", color = Color3.fromRGB(255, 255, 255), message = "Eyes appeared! Don't look at them!"},
        {name = "Screech", displayName = "Screech", color = Color3.fromRGB(0, 255, 0), message = "Screech is near! Turn around when you hear the sound!"},
        {name = "Halt", displayName = "Halt", color = Color3.fromRGB(0, 0, 255), message = "Halt appeared! Stop moving when the screen turns blue!"},
        {name = "Seek", displayName = "Seek", color = Color3.fromRGB(0, 0, 0), message = "Seek chase sequence! Run and avoid obstacles!"},
        {name = "Figure", displayName = "Figure", color = Color3.fromRGB(139, 0, 255), message = "Figure is in the library! Move quietly and avoid making noise!"},
        {name = "Dupe", displayName = "Dupe", color = Color3.fromRGB(255, 255, 0), message = "Dupe detected! Check door numbers carefully!"},
        {name = "A60", displayName = "A-60", color = Color3.fromRGB(255, 0, 255), message = "A-60 is coming! Hide in a locker immediately!"},
        {name = "A120", displayName = "A-120", color = Color3.fromRGB(0, 255, 255), message = "A-120 detected! Find a hiding spot quickly!"}
    }
    
    local function onEntityAdded(entity)
        for _, entityData in pairs(entities) do
            if entity.Name == entityData.name or entity.Name:find(entityData.name) then
                local currentTime = tick()
                
                -- Prevent spam notifications
                if lastEntityNotification[entityData.name] and currentTime - lastEntityNotification[entityData.name] < 3 then
                    return
                end
                lastEntityNotification[entityData.name] = currentTime
                
                -- Check if we should notify for this entity
                local shouldNotify = false
                if allEntityNotifications then
                    shouldNotify = true
                elseif rushNotifEnabled and (entityData.name == "RushMoving" or entityData.name == "AmbushMoving") then
                    shouldNotify = true
                end
                
                if shouldNotify then
                    createAdvancedNotification(
                        "‚ö†Ô∏è ENTITY DETECTED: " .. entityData.displayName,
                        entityData.message,
                        entityData.color,
                        6,
                        true
                    )
                    
                    print("üö® Entity Alert: " .. entityData.displayName .. " detected!")
                end
                break
            end
        end
    end
    
    local conn = workspace.DescendantAdded:Connect(function(descendant)
        if descendant:IsA("Model") then
            task.wait(0.1)
            onEntityAdded(descendant)
        end
    end)
    
    table.insert(entityConnections, conn)
end

-- ============================================================================
-- ADVANCED AUTO UNLOCK SYSTEM
-- ============================================================================

local autoUnlockConnection

local function enableAutoUnlock()
    autoUnlockEnabled = true
    
    if autoUnlockConnection then autoUnlockConnection:Disconnect() end
    
    local function unlockDoors()
        if not autoUnlockEnabled then return end
        
        local currentRooms = workspace:FindFirstChild("CurrentRooms")
        if not currentRooms then return end
        
        for _, room in pairs(currentRooms:GetChildren()) do
            if room:IsA("Model") and tonumber(room.Name) then
                local door = room:FindFirstChild("Door")
                if door then
                    local lockPart = door:FindFirstChild("Lock")
                    if lockPart then
                        local prompt = lockPart:FindFirstChildOfClass("ProximityPrompt")
                        if prompt and prompt.Enabled and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            local distance = (player.Character.HumanoidRootPart.Position - lockPart.Position).Magnitude
                            if distance <= 15 then
                                pcall(function()
                                    fireproximityprompt(prompt)
                                end)
                            end
                        end
                    end
                end
            end
        end
    end
    
    autoUnlockConnection = RunService.Heartbeat:Connect(function()
        unlockDoors()
    end)
    
    print("üîì Advanced Auto Unlock: ENABLED")
end

local function disableAutoUnlock()
    autoUnlockEnabled = false
    
    if autoUnlockConnection then autoUnlockConnection:Disconnect() end
    
    print("üîì Advanced Auto Unlock: DISABLED")
end

-- ============================================================================
-- INFINITE VITAMINS SYSTEM
-- ============================================================================

local infVitaminsConnection

local function enableInfVitamins()
    infVitaminsEnabled = true
    
    if infVitaminsConnection then infVitaminsConnection:Disconnect() end
    
    local function maintainVitamins()
        if not infVitaminsEnabled or not player.Character then return end
        
        local humanoid = player.Character:FindFirstChild("Humanoid")
        if not humanoid then return end
        
        -- Keep vitamins effect active
        for _, effect in pairs(humanoid:GetChildren()) do
            if effect:IsA("NumberValue") and effect.Name == "Vitamins" then
                effect.Value = math.huge
            end
        end
        
        -- Auto-use vitamins when health is low
        if humanoid.Health < humanoid.MaxHealth * 0.8 then
            for _, item in pairs(player.Backpack:GetChildren()) do
                if item.Name == "Vitamins" and item:IsA("Tool") then
                    humanoid:EquipTool(item)
                    task.wait(0.5)
                    if item:FindFirstChild("Handle") then
                        local prompt = item.Handle:FindFirstChildOfClass("ProximityPrompt")
                        if prompt then
                            fireproximityprompt(prompt)
                        end
                    end
                    break
                end
            end
        end
    end
    
    infVitaminsConnection = RunService.Heartbeat:Connect(function()
        maintainVitamins()
    end)
    
    print("üíä Infinite Vitamins: ENABLED")
end

local function disableInfVitamins()
    infVitaminsEnabled = false
    
    if infVitaminsConnection then infVitaminsConnection:Disconnect() end
    
    print("üíä Infinite Vitamins: DISABLED")
end

