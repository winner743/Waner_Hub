    addBookESP()
    if bookConnection then bookConnection:Disconnect() end
    bookConnection = workspace.DescendantAdded:Connect(function(v)
        if bookEspEnabled and v:IsA("Model") and (v.Name == "LiveHintBook" or v.Name == "Book") then
            task.wait(0.5)
            addBookESP()
